<?xml version="1.0" encoding="utf-8"?>
<job minFilesPerProcess="1" maxFilesPerProcess="1" filesPerHour="1" simulateSubmission="false" fileListSyntax="xrootd">


  <command>
    starver SL21d
    root4star -q -b Run12pp200Ana.C\(\"$INPUTFILE0\"\,\"pidhist_${FILEBASENAME}.root\"\)
  </command>

  <SandBox installer="ZIP">
    <Package>
      <File>file:./Run12pp200Ana.C</File> <!-- Required -->
      <File>file:./StRoot/*/*</File> <!-- Required -->
      <File>file:./.sl73_gcc485</File> <!-- Required -->
    </Package>
  </SandBox>


  <input URL="filelist:/star/u/pokhrel/DihadronCrossSection/PID/trees.list" />
  <stdout URL="file:/star/u/pokhrel/DihadronCrossSection/PID/logFiles/$FILEBASENAME.out" />
  <stderr URL="file:/star/u/pokhrel/DihadronCrossSection/PID/logFiles/$FILEBASENAME.err" />

  <output fromScratch="*.root" toURL="/star/u/pokhrel/DihadronCrossSection/PID/Hist/" />


  <Generator>
    <Location>/star/u/pokhrel/DihadronCrossSection/PID/subFiles/</Location>
    <ReportLocation>/star/u/pokhrel/DihadronCrossSection/PID/subFiles/</ReportLocation>
  </Generator>
</job>
